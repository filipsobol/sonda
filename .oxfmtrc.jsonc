{
	"$schema": "./node_modules/oxfmt/configuration_schema.json",
	"printWidth": 120,
	"bracketSpacing": true,
	"semi": true,
	"singleQuote": true,
	"singleAttributePerLine": true,
	"useTabs": true,
	"trailingComma": "none",
	"bracketSameLine": false,
	"arrowParens": "avoid",
	"objectWrap": "preserve",
	"ignorePatterns": ["docs/public/demo.html"],
	"experimentalSortPackageJson": true,
	"experimentalTailwindcss": {}
}
